  "bugs": {
    "url": "https://github.com/mervynyang/json-api-fetch/issues"
  },
  "homepage": "https://github.com/mervynyang/json-api-fetch#readme",
  "devDependencies": {
    "@types/jest": "^24.0.21",
    "cpr": "^3.0.1",
    "express": "^4.17.1",
    "jest": "^24.9.0",
    "rollup": "^1.27.8",
    "shx": "^0.3.2",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  }
